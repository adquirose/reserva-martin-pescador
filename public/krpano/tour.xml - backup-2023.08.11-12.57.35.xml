		<view hlookat="-349.954" vlookat="5.939" fovtype="MFOV" fov="100.200" maxpixelzoom="2.0" fovmin="70" fovmax="130" limitview="range" hlookatmin="-180" hlookatmax="180" vlookatmin="-33" vlookatmax="90"
 />

		<preview url="panos/3.tiles/preview.jpg" />

		<image>
			<cube url="panos/3.tiles/pano_%s.jpg" />
		</image>

