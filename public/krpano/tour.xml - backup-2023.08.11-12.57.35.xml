<krpano version="1.18" title="Chupalla 27" onstart="startup();">

	<include url="skin/vtourskin.xml" />
	<include url="skin/vistageneral.xml" />
	<include url="plugins/showtext.xml" />
     <include url="skin/hotspots-actions.xml" />
    <include url="skin/spots.xml" />
<include url="skin/vtourskin_design_117.xml" />

   
	<!-- set skin settings: bingmaps? gyro? thumbnails? tooltips? -->
	<skin_settings maps="false"
	               maps_type="bing"
	               maps_bing_api_key=""
	               maps_zoombuttons="false"
	               gyro="true"
	               title="true"
	               thumbs="true"
	               thumbs_width="120" thumbs_height="55" thumbs_padding="10" thumbs_crop="0|40|240|160"
	               thumbs_opened="true"
	               thumbs_text="true"
	               thumbs_dragging="true"
	               thumbs_onhoverscrolling="false"
	               thumbs_scrollbuttons="false"
	               thumbs_scrollindicator="false"
	               thumbs_loop="false"
	               tooltips_thumbs="false"
	               tooltips_hotspots="false"
	               tooltips_mapspots="false"
	               loadscene_flags="MERGE"
	               loadscene_blend="BLEND(0.5)"
	               controlbar_offset="20"
	               />


	<!-- set optional skin logo url -->


	   <style name="oli"
        scale="0.5"
        zorder="50"
        handcursor="true"
        onhover="showtext(Disponible · Ver ficha);"
        alpha="1"
        type="text"
        edge="center"
        css="font-family:Din Alternate; font-size:21px; color:#000000;font-weight: bold; text-align:center;"
        vcenter="true"
        padding="5"
        bg="true"
        bgcolor="0xffffff"
        bgalpha="1.0"
        bgborder="0"
        bgroundedge="22"
        bgshadow=""
        txtshadow=""
        width="44" height="44"
    />

	  <style name="spot-andrea" scale="0.4" zorder="50" edge="bottom" align="bottom" alpha="1"
        capture="false" handcursor="false"
	    fillcolor="0xffff00" fillalpha="0.30"
	    bordercolor="0xffff00" borderalpha="0.80"
	    onover="tween(alpha, 0.1, 0.25);"
	    onout="tween(alpha, 1.0, 0.25);"
	    ondown.touch="asyncloop(pressed, onhover(); );"
	    onup.touch="onout();"
    />

     
     <style name="skin_hotspotstyle4" scale="0.5" handcursor="false" edge="bottomleft" align="bottom" alpha="1"/>
  


	 <style name="skin_hotspotstyle2" scale="0.6" distorted="false" oy="0" handcursor="true" zorder="50" />


	 <textstyle name="default"



	           font="Helvetica" fontsize.no-mobile="14" fontsize.mobile="14" bold="false"



	           background="true" backgroundcolor="0x211915" backgroundalpha="0.667"



	           border="true" borderwidth="2" bordercolor="0xFFFFFF" roundedge="1"



	           textcolor="0xFFFFFF"



	           padding="6 10"



	           textalign="center"



	           yoffset.no-touch="-5"



	           yoffset.touch="-40"



	           />

	<action name="startup">
		if(startscene === null, copy(startscene,scene[0].name));
		loadscene(get(startscene), null, MERGE);
	</action>


	 <scene name="scene_master" title="Masterplan 360º" onstart="mostrar_btn_vistageneral(false);generar_spots(1,79)" thumburl="panos/master.tiles/thumb.jpg" lat="" lng="" heading="">

		<view hlookat="-349.258" vlookat="28.909" fovtype="MFOV" fov="107.662" maxpixelzoom="2.0" fovmin="70" fovmax="130" limitview="range" hlookatmin="-180" hlookatmax="180" vlookatmin="-33" vlookatmax="90"
 />

		<preview url="panos/master.tiles/preview.jpg" />

		<image>
			<cube url="panos/master.tiles/pano_%s.jpg" />
		</image>

		<!-- place your scene hotspots here -->
	<hotspot name="s1" url="skin/spots/lupa.png" style="skin_hotspotstyle2" onclick="looktohotspot(s1, 70, tween(easeOutQuad,3));cargar_escena(scene_1);" onhover="showtext(Ver Vista 1);" ath="-48.729" atv="36.720"/>
	<hotspot name="s2" url="skin/spots/lupa.png" style="skin_hotspotstyle2" onclick="looktohotspot(s2, 70, tween(easeOutQuad,3));cargar_escena(scene_2);" onhover="showtext(Ver Vista 2);" ath="-0.242" atv="37.297"/>
	<hotspot name="s3" url="skin/spots/lupa.png" style="skin_hotspotstyle2" onclick="looktohotspot(s3, 70, tween(easeOutQuad,3));cargar_escena(scene_3);" onhover="showtext(Ver Vista 3);" ath="19.598" atv="52.711"/>



    <hotspot name="hito1" style="skin_hotspotstyle4" url="skin/spots/1.png" ath="-6.045" atv="3.644"/>
	<hotspot name="hito3" style="skin_hotspotstyle4" url="skin/spots/3.png" ath="58.383" atv="-0.469"/>
	<hotspot name="hito2" style="skin_hotspotstyle4" url="skin/spots/2.png" ath="-51.938" atv="1.871"/>
	<hotspot name="hito4" style="skin_hotspotstyle4" url="skin/spots/4.png" ath="71.311" atv="-0.432"/>
	<hotspot name="hito5" style="skin_hotspotstyle4" url="skin/spots/5.png" ath="-162.086" atv="-0.799"/>



    <hotspot name="spot2" handcursor="false" style="spot-andrea" url="skin/spots/acceso.png" onhover="showtext(Acceso);" ath="28.027" atv="35.439"/>
 

 




	</scene>

