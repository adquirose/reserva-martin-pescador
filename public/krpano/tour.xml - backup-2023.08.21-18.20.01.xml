<krpano version="1.18" title="Nativo Tongoy" onstart="startup();">

	<include url="skin/vtourskin.xml" />
	<include url="skin/vistageneral.xml" />
	<include url="plugins/showtext.xml" />
     <include url="skin/hotspots-actions.xml" />
    <include url="skin/spots.xml" />
<include url="skin/vtourskin_design_117.xml" />
    <include url="skin/ficha.xml" />
   
	<!-- set skin settings: bingmaps? gyro? thumbnails? tooltips? -->
	<skin_settings maps="false"
	               maps_type="bing"
	               maps_bing_api_key=""
	               maps_zoombuttons="false"
	               gyro="true"
	               title="true"
	               thumbs="true"
	               thumbs_width="120" thumbs_height="55" thumbs_padding="10" thumbs_crop="0|40|240|160"
	               thumbs_opened="true"
	               thumbs_text="true"
	               thumbs_dragging="true"
	               thumbs_onhoverscrolling="false"
	               thumbs_scrollbuttons="false"
	               thumbs_scrollindicator="false"
	               thumbs_loop="false"
	               tooltips_thumbs="false"
	               tooltips_hotspots="false"
	               tooltips_mapspots="false"
	               loadscene_flags="MERGE"
	               loadscene_blend="BLEND(0.5)"
	               controlbar_offset="20"
	               />


	<!-- set optional skin logo url -->


	   <style name="oli"
        scale="0.5"
        zorder="50"
        handcursor="true"
        onhover="showtext(Disponible · Ver ficha);"
        alpha="1"
        type="text"
        edge="center"
        css="font-family:Din Alternate; font-size:21px; color:#000000;font-weight: bold; text-align:center;"
        vcenter="true"
        padding="5"
        bg="true"
        bgcolor="0xffffff"
        bgalpha="1.0"
        bgborder="0"
        bgroundedge="22"
        bgshadow=""
        txtshadow=""
        width="44" height="44"
    />

	  <style name="spot-andrea" scale="0.4" zorder="50" edge="bottom" align="bottom" alpha="1"
        capture="false" handcursor="false"
	    fillcolor="0xffff00" fillalpha="0.30"
	    bordercolor="0xffff00" borderalpha="0.80"
	    onover="tween(alpha, 0.1, 0.25);"
	    onout="tween(alpha, 1.0, 0.25);"
	    ondown.touch="asyncloop(pressed, onhover(); );"
	    onup.touch="onout();"
    />

     
     <style name="skin_hotspotstyle4" scale="0.5" handcursor="false" edge="bottomleft" align="bottom" alpha="1"/>
  


	 <style name="lupa" scale="0.6" distorted="false" oy="0" handcursor="true" zorder="50" />


	 <textstyle name="default"



	           font="Helvetica" fontsize.no-mobile="14" fontsize.mobile="14" bold="false"



	           background="true" backgroundcolor="0x211915" backgroundalpha="0.667"



	           border="true" borderwidth="2" bordercolor="0xFFFFFF" roundedge="1"



	           textcolor="0xFFFFFF"



	           padding="6 10"



	           textalign="center"



	           yoffset.no-touch="-5"



	           yoffset.touch="-40"



	           />

	<action name="startup">
		if(startscene === null, copy(startscene,scene[0].name));
		loadscene(get(startscene), null, MERGE);
	</action>


	 <scene name="scene_master" title="Masterplan 360º" onstart="mostrar_btn_vistageneral(false);generar_spots(1,78)" thumburl="panos/master.tiles/thumb.jpg" lat="" lng="" heading="">

		<view hlookat="-22.814" vlookat="24.736" fovtype="MFOV" fov="103.167" maxpixelzoom="2.0" fovmin="70" fovmax="130" limitview="range" hlookatmin="-180" hlookatmax="180" vlookatmin="-33" vlookatmax="90"
 />

		<preview url="panos/master.tiles/preview.jpg" />

		<image>
			<cube url="panos/master.tiles/pano_%s.jpg" />
		</image>

		<!-- place your scene hotspots here -->
	<hotspot name="s1" url="skin/spots/lupa.png" style="lupa" onclick="looktohotspot(s1, 70, tween(easeOutQuad,3));cargar_escena(scene_1);" onhover="showtext(Ver Vista 1);" ath="61.285" atv="66.866"/>
	<hotspot name="s2" url="skin/spots/lupa.png" style="lupa" onclick="looktohotspot(s2, 70, tween(easeOutQuad,3));cargar_escena(scene_2);" onhover="showtext(Ver Vista 2);" ath="158.349" atv="70.534"/>
	<hotspot name="s3" url="skin/spots/lupa.png" style="lupa" onclick="looktohotspot(s3, 70, tween(easeOutQuad,3));cargar_escena(scene_3);" onhover="showtext(Ver Vista 3);" ath="-82.692" atv="50.897"/>



    <hotspot name="hito1" style="skin_hotspotstyle4" url="skin/spots/1.png" ath="-35.971" atv="3.203"/>
	<hotspot name="hito3" style="skin_hotspotstyle4" url="skin/spots/3.png" ath="34.728" atv="-1.283"/>
	<hotspot name="hito2" style="skin_hotspotstyle4" url="skin/spots/2.png" ath="-84.609" atv="1.617"/>
	<hotspot name="hito4" style="skin_hotspotstyle4" url="skin/spots/4.png" ath="44.959" atv="-1.012"/>
	<hotspot name="hito5" style="skin_hotspotstyle4" url="skin/spots/5.png" ath="98.676" atv="1.037"/>
	<hotspot name="hito6" style="skin_hotspotstyle4" url="skin/spots/6.png" ath="-45.547" atv="11.985"/>
	<hotspot name="hito7" style="skin_hotspotstyle4" url="skin/spots/7.png" ath="-106.920" atv="1.040"/>
	<hotspot name="hito9" style="skin_hotspotstyle4" url="skin/spots/9.png" ath="34.134" atv="20.031"/>


    <hotspot name="spot1" handcursor="false" style="spot-andrea" url="skin/spots/acceso.png" onhover="showtext(Acceso);" ath="54.992" atv="48.100"/>
     <hotspot name="spot2" handcursor="false" style="spot-andrea" url="skin/spots/acceso.png" onhover="showtext(Acceso);" ath="176.828" atv="46.983"/>
     <hotspot name="spot3" handcursor="false" style="spot-andrea" url="skin/spots/acceso.png" onhover="showtext(Acceso Principal);" ath="143.194" atv="62.794"/>
     <hotspot name="spot4" handcursor="false" style="spot-andrea" url="skin/spots/acceso.png" onhover="showtext(Acceso);" ath="76.272" atv="60.676"/>
      <hotspot name="spot5" handcursor="false" style="spot-andrea" url="skin/spots/agua.png" onhover="showtext(Futura Planta de Agua);" ath="-5.372" atv="36.753"/>
    <hotspot name="spot6" handcursor="false" style="spot-andrea" url="skin/spots/reserva.png" onhover="showtext(Reserva Natural);" ath="7.663" atv="40.018"/>
 






	</scene>

